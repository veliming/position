/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.welcome_title{
  text-align: center;
  font-size: 40rpx;
  margin-top:30px;
}
.welcome_content{
  padding: 30rpx 0;
  text-align: center;
  color: #bababa;
  border-bottom: 1px solid #dfd9d9;
}
.userLicenseAgreement>button{
  font-size: 40rpx;
  color:#fff;
}
.userLicenseAgreement>button::after{
  border:none;
}
.userLicenseAgreementBox,.repairFeedbackBox{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color:rgba(0,0,0,.5);
  display: flex;
  align-items: center;
  justify-content: center;
}
.userLicenseAgreement{
  position: relative;
  height: 260px;
  background-color: #fff;
  border-radius: 15rpx;
}
.userLicenseAgreementTitle{
  margin:20px 0;
  text-align: center;
  font-size: 20px;
}
.userLicenseAgreementExplain{
  text-indent: 1rem;
  padding:0 20px;
  font-size: 16px;
}
.userLicenseAgreementLink{
  margin: 25px 0 20px 0;
  color: rgb(132,157,293);
  text-align: center;
  font-size: 14px;
}
.userLicenseAgreementBtn{
 font-size: 16px;
 margin:20px;
 margin-top: 15px;
}